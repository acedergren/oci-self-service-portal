{
	"name": "@portal/server",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"check": "tsc --noEmit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix"
	},
	"exports": {
		".": "./src/index.ts",
		"./admin": "./src/admin/index.ts",
		"./admin/*.js": "./src/admin/*.ts",
		"./admin/*": "./src/admin/*.ts",
		"./agent-state": "./src/agent-state/index.ts",
		"./agent-state/*.js": "./src/agent-state/*.ts",
		"./agent-state/*": "./src/agent-state/*.ts",
		"./auth": "./src/auth/index.ts",
		"./auth/*.js": "./src/auth/*.ts",
		"./auth/*": "./src/auth/*.ts",
		"./mcp-client": "./src/mcp-client/index.ts",
		"./mcp-client/*.js": "./src/mcp-client/*.ts",
		"./mcp-client/*": "./src/mcp-client/*.ts",
		"./mcp-client/transports": "./src/mcp-client/transports/index.ts",
		"./mcp-client/transports/*.js": "./src/mcp-client/transports/*.ts",
		"./mcp-client/transports/*": "./src/mcp-client/transports/*.ts",
		"./mcp-client/configs/*.js": "./src/mcp-client/configs/*.ts",
		"./mcp-client/configs/*": "./src/mcp-client/configs/*.ts",
		"./oracle": "./src/oracle/index.ts",
		"./oracle/*.js": "./src/oracle/*.ts",
		"./oracle/*": "./src/oracle/*.ts",
		"./oracle/repositories": "./src/oracle/repositories/index.ts",
		"./oracle/repositories/*.js": "./src/oracle/repositories/*.ts",
		"./oracle/repositories/*": "./src/oracle/repositories/*.ts",
		"./*.js": "./src/*.ts",
		"./*": "./src/*.ts"
	},
	"dependencies": {
		"@portal/types": "workspace:*",
		"ipaddr.js": "^2.2.0",
		"@acedergren/oci-genai-provider": "^0.2.0",
		"@modelcontextprotocol/sdk": "^1.26.0",
		"ai": "^6.0.69",
		"better-auth": "^1.4.18",
		"better-sqlite3": "^12.6.2",
		"oci-sdk": "^2.125.0",
		"oracledb": "^6.10.0",
		"pino": "^10.3.0",
		"uuid": "^13.0.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^25.2.3",
		"typescript": "^5.9.3"
	},
	"peerDependencies": {
		"@sveltejs/kit": "^2.50.2"
	}
}
