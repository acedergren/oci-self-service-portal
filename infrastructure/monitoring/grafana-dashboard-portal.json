				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "portal_db_pool_active{environment=\"$environment\"} / (portal_db_pool_active{environment=\"$environment\"} + portal_db_pool_idle{environment=\"$environment\"})",
					"legendFormat": "utilization",
