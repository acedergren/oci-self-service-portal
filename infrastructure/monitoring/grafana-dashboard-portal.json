				"legend": { "displayMode": "table", "placement": "right" },
				"tooltip": { "mode": "multi" }
			},
			"title": "Tool Failure Rate by Tool",
			"type": "timeseries",
			"targets": [
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "sum by (tool) (rate(portal_tool_executions_total{success=\"false\",environment=\"$environment\"}[5m])) / sum by (tool) (rate(portal_tool_executions_total{environment=\"$environment\"}[5m]))",
					"legendFormat": "{{tool}}",
					"refId": "A"
				}
			]
		},
		{
			"collapsed": false,
