			"type": "barchart",
			"targets": [
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "topk(10, sum by (tool) (increase(portal_tool_executions_total{environment=\"$environment\"}[$__range])))",
					"legendFormat": "{{tool}}",
					"refId": "A",
					"instant": true
				}
			]
		},
		{
			"datasource": { "type": "prometheus", "uid": "${datasource}" },
			"fieldConfig": {
				"defaults": {
					"color": { "mode": "thresholds" },
					"thresholds": {
						"mode": "absolute",
						"steps": [
							{ "color": "green", "value": null },
							{ "color": "yellow", "value": 0.01 },
							{ "color": "red", "value": 0.05 }
