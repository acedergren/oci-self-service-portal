			"gridPos": { "h": 8, "w": 8, "x": 16, "y": 25 },
			"id": 11,
			"options": {
				"legend": { "displayMode": "list", "placement": "bottom" },
				"tooltip": { "mode": "multi" }
			},
			"title": "HTTP Request Duration (p50 / p95)",
			"type": "timeseries",
			"targets": [
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "histogram_quantile(0.50, sum by (le) (rate(portal_http_request_duration_seconds_bucket{environment=\"$environment\"}[5m])))",
					"legendFormat": "p50",
					"refId": "A"
				},
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "histogram_quantile(0.95, sum by (le) (rate(portal_http_request_duration_seconds_bucket{environment=\"$environment\"}[5m])))",
					"legendFormat": "p95",
