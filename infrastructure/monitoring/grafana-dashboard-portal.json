			"title": "Tools",
			"type": "row"
		},
		{
			"datasource": { "type": "prometheus", "uid": "${datasource}" },
			"fieldConfig": {
				"defaults": {
					"color": { "mode": "palette-classic" },
					"custom": {
						"axisCenteredZero": false,
						"axisLabel": "executions/s",
						"drawStyle": "line",
						"fillOpacity": 20,
						"lineWidth": 2,
						"pointSize": 5,
						"showPoints": "auto",
						"stacking": { "mode": "none" }
					},
					"unit": "ops"
				}
			},
			"gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
			"id": 5,
			"options": {
				"legend": { "displayMode": "table", "placement": "right" },
				"tooltip": { "mode": "multi" }
			},
			"title": "Tool Executions by Category",
			"type": "timeseries",
			"targets": [
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "sum by (category) (rate(portal_tool_executions_total{environment=\"$environment\"}[5m]))",
					"legendFormat": "{{category}}",
					"refId": "A"
				}
			]
		},
		{
			"datasource": { "type": "prometheus", "uid": "${datasource}" },
			"fieldConfig": {
				"defaults": {
					"color": { "mode": "palette-classic" },
					"custom": {
						"axisCenteredZero": false,
						"axisLabel": "latency (s)",
						"drawStyle": "line",
						"fillOpacity": 10,
						"lineWidth": 2
					},
					"unit": "s"
				}
			},
			"gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
			"id": 6,
			"options": {
				"legend": { "displayMode": "list", "placement": "bottom" },
				"tooltip": { "mode": "multi" }
			},
			"title": "Tool Latency (p50 / p95 / p99)",
			"type": "timeseries",
			"targets": [
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "histogram_quantile(0.50, sum by (le) (rate(portal_tool_duration_seconds_bucket{environment=\"$environment\"}[5m])))",
					"legendFormat": "p50",
					"refId": "A"
				},
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "histogram_quantile(0.95, sum by (le) (rate(portal_tool_duration_seconds_bucket{environment=\"$environment\"}[5m])))",
					"legendFormat": "p95",
					"refId": "B"
				},
				{
					"datasource": { "type": "prometheus", "uid": "${datasource}" },
					"expr": "histogram_quantile(0.99, sum by (le) (rate(portal_tool_duration_seconds_bucket{environment=\"$environment\"}[5m])))",
					"legendFormat": "p99",
					"refId": "C"
				}
			]
		},
		{
			"datasource": { "type": "prometheus", "uid": "${datasource}" },
			"fieldConfig": {
